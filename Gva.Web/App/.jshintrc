{  
  "es3": true,
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxlen": 100,
  "quotmark": "single",
  "-W100": true // BOM markers throw a 'This character may get silently deleted by one or more browsers.' message
}
