	<xsd:import namespace="http://ereg.egov.bg/segment/0009-000139" />
	<xsd:import namespace="http://ereg.egov.bg/segment/0009-000153" />	
	
	<xsd:element name="ReplacementRemovalRestrictionsLicenseManagementApplication" type="ReplacementRemovalRestrictionsLicenseManagementApplication"/>
	<xsd:complexType name="ReplacementRemovalRestrictionsLicenseManagementApplication">
		<xsd:annotation>
			<xsd:documentation xml:lang="bg">Заявление за замяна/подмяна/премахване на ограничения на  свидетелство за правоспособност на инженеро-технически състав, извършващ техническо обслужване на средствата за управление на въздушното движение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ElectronicAdministrativeServiceHeader" type="eash:ElectronicAdministrativeServiceHeader" minOccurs="0" />
			<xsd:element name="CAAPersonalIdentificationNumber" type="cpin:CAAPersonalIdentificationNumber" minOccurs="0" />
			<xsd:element name="AviationSecurityCertificateNumber" type="ascn:AviationSecurityCertificateNumber" minOccurs="0" />
			<xsd:element name="AviationSecurityCertificateDate" type="ascd:AviationSecurityCertificateDate" minOccurs="0" />
			<xsd:element name="PersonBasicData" type="pbd:PersonBasicData" minOccurs="0" />
			<xsd:element name="PlaceOfBirth" type="pob:PlaceOfBirth" minOccurs="0" />
			<xsd:element name="ForeignCitizenBasicData" type="fcbd:ForeignCitizenBasicData" minOccurs="0" />
			<xsd:element name="SPNumber" type="sn:SPNumber" minOccurs="0" />
			<xsd:element name="LicenseDegree" type="ld:LicenseDegree" minOccurs="0" />
			<xsd:element name="LicenseChangeType" type="lct:LicenseChangeType" minOccurs="0" />
			<xsd:element name="ChangeLicenseRestrictionsDescription" type="clrd:ChangeLicenseRestrictionsDescription" minOccurs="0" />
			<xsd:element name="RemovedLicensesRestrictions" type="rlr:RemovedLicensesRestrictions" minOccurs="0" />
			<xsd:element name="AviationSecurityExperiences" type="ase:AviationSecurityExperiences" minOccurs="0" />
			<xsd:element name="Recognition" type="r:Recognition" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="EntityBasicData" type="ebd:EntityBasicData" minOccurs="0" />
			<xsd:element name="ForeignEntityBasicData" type="febd:ForeignEntityBasicData" minOccurs="0" />
			<xsd:element name="ContactInformation" type="ci:ContactInformation" minOccurs="0" />
			<xsd:element name="CandidateKnowledge" type="ck:CandidateKnowledge" minOccurs="0" />
			<xsd:element name="RecommendationAviationSecurity" type="ras:RecommendationAviationSecurity" minOccurs="0" />
			<xsd:element name="AttachedDocumentDatasCollection" type="addc:AttachedDocumentDatasCollection" minOccurs="0" />
			<xsd:element name="EAURecipientsAttachedDocumentDatasCollection" type="eraddc:EAURecipientsAttachedDocumentDatasCollection" minOccurs="0" />
			<xsd:element name="ConsentReceivingElectronicStatements" type="cres:ConsentReceivingElectronicStatements" minOccurs="0" />
			<xsd:element name="AttachedDocuments" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AttachedDocument" type="ad:AttachedDocument"  maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ElectronicAdministrativeServiceFooter" type="easf:ElectronicAdministrativeServiceFooter" minOccurs="0"/>			
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>