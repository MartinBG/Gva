			<xsd:element name="FacilityManufacturerName" type="fmn:FacilityManufacturerName" minOccurs="0" />
			<xsd:element name="CountryGRAOCode" type="cgc:CountryGRAOCode" minOccurs="0" />
			<xsd:element name="CountryName" type="cn:CountryName" minOccurs="0" />
			<xsd:element name="FacilityManufactureDate" type="fmd:FacilityManufactureDate" minOccurs="0" />
			<xsd:element name="FacilityPlaceAcceptanceDate" type="fpad:FacilityPlaceAcceptanceDate" minOccurs="0" />
