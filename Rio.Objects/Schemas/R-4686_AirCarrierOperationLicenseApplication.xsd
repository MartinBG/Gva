			<xsd:element name="AircraftServicesPerform" type="asp:AircraftServicesPerform" minOccurs="0" />
			<xsd:element name="OperatingLicenseStartDate" type="olsd:OperatingLicenseStartDate" minOccurs="0" />
			<xsd:element name="Options" type="o:Options" minOccurs="0" />
			<xsd:element name="ShareholdersOwnersCollection" type="soc:ShareholdersOwnersCollection" minOccurs="0" />
			<xsd:element name="MotherCompanyInformation" type="mci:MotherCompanyInformation" minOccurs="0" />
			<xsd:element name="HoldingCompanyInformation" type="hci:HoldingCompanyInformation" minOccurs="0" />
			<xsd:element name="CandidateRepresentativesPersonRepresentingTradingCompanyData" type="prtcd:PersonsRepresentingTradingCompaniesDatasCollection" minOccurs="0" />
			<xsd:element name="MotherCompanyRepresentativesPersonRepresentingTradingCompanyData" type="prtcd:PersonsRepresentingTradingCompaniesDatasCollection" minOccurs="0" />
