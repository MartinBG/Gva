			<xsd:element name="ForeignCitizenBasicData" type="fcbd:ForeignCitizenBasicData" minOccurs="0" />
			<xsd:element name="QualificationClassDetails" type="qcd:QualificationClassDetails" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="AviationSecurityExperiences" type="ase:AviationSecurityExperiences" minOccurs="0" />
			<xsd:element name="Permission" type="p:Permission" minOccurs="0" maxOccurs="unbounded" />
