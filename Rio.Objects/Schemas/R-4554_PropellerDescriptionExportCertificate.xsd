<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-4554" 
	xmlns="http://ereg.egov.bg/segment/R-4554"
	xmlns:mn="http://ereg.egov.bg/value/R-3986"
	xmlns:pt="http://ereg.egov.bg/segment/R-4434"
	xmlns:asntc="http://ereg.egov.bg/segment/R-4548"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified">

	<xsd:import namespace="http://ereg.egov.bg/value/R-3986" />
	<xsd:import namespace="http://ereg.egov.bg/segment/R-4434" />
	<xsd:import namespace="http://ereg.egov.bg/segment/R-4548" />

	<xsd:complexType name="PropellerDescriptionExportCertificate">
		<xsd:annotation>
			<xsd:documentation xml:lang="bg">Описание на витло за експортно удостоверение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ManufacturerName" type="mn:ManufacturerName" minOccurs="0" />
			<xsd:element name="PropellerType" type="pt:PropellerType" minOccurs="0" />
			<xsd:element name="AircraftsSerialNumbersTTSNCollection" type="asntc:AircraftsSerialNumbersTTSNCollection" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
