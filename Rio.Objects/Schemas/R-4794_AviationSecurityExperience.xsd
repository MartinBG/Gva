<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-4794" 
	xmlns="http://ereg.egov.bg/segment/R-4794" 
	xmlns:pesd="http://ereg.egov.bg/value/R-4784"
	xmlns:peed="http://ereg.egov.bg/value/R-4786"
	xmlns:od="http://ereg.egov.bg/value/R-4788"
	xmlns:op="http://ereg.egov.bg/value/R-4790"
	xmlns:jd="http://ereg.egov.bg/value/R-4792"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
