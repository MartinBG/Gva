<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/0009-000009" xmlns="http://ereg.egov.bg/segment/0009-000009" xmlns:ctlc="http://ereg.egov.bg/value/0008-000024" xmlns:cnc="http://ereg.egov.bg/value/0008-000025" xmlns:san="http://ereg.egov.bg/value/0008-000023" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
