    <xsd:annotation>
      <xsd:documentation xml:lang="bg">Потвърждаване за получаване</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ElectronicServiceProvider" type="espbd:ElectronicServiceProviderBasicData" minOccurs="0" />
