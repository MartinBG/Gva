<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-1122" xmlns="http://ereg.egov.bg/segment/R-1122" 
	xmlns:est="http://ereg.egov.bg/value/R-1078" 
	xmlns:epr="http://ereg.egov.bg/value/R-1108" 
