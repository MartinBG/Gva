	xmlns:iwn="http://ereg.egov.bg/value/R-1116" 
	xmlns:sime="http://ereg.egov.bg/value/R-1118" 
	xmlns:simea="http://ereg.egov.bg/value/R-1120" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	
    <xsd:import namespace="http://ereg.egov.bg/value/R-1078" />
    <xsd:import namespace="http://ereg.egov.bg/value/R-1108" />
    <xsd:import namespace="http://ereg.egov.bg/value/R-1110" />
