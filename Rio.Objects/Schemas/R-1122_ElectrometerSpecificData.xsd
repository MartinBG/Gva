    <xsd:annotation>
      <xsd:documentation xml:lang="bg">Специфични данни за електромер</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ElectrometerSpecificType" type="est:ElectrometerSpecificType" minOccurs="0" />
