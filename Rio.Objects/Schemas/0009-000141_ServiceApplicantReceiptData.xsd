              <xsd:element name="AreaName" type="an:AreaName" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="PostOfficeBox" type="pob:PostOfficeBox" />
      </xsd:choice>
    </xsd:sequence>
