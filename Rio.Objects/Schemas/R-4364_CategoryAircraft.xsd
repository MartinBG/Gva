	elementFormDefault="qualified">
	
	<xsd:import namespace="http://ereg.egov.bg/value/R-4360" />
	<xsd:import namespace="http://ereg.egov.bg/value/R-4362" />
		
	<xsd:complexType name="CategoryAircraft">
		<xsd:annotation>
			<xsd:documentation xml:lang="bg">Категория на въздухоплавателно средство</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CategoryAircraftCode" type="cac:CategoryAircraftCode" minOccurs="0" />
