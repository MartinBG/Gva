<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-4364" 
	xmlns="http://ereg.egov.bg/segment/R-4364" 
	xmlns:cac="http://ereg.egov.bg/value/R-4360"
	xmlns:can="http://ereg.egov.bg/value/R-4362"
